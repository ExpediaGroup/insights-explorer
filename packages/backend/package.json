{
  "name": "@iex/backend",
  "version": "0.0.0",
  "description": "Insights Explorer Backend",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist && rm -f tsconfig.tsbuildinfo",
    "clean:deep": "rm -rf node_modules",
    "----------": "---------------------------------------",
    "build": "run-s build:tsc",
    "build:tsc": "tsc --build --incremental --verbose",
    "build:tsc:watch": "tsc --build --watch",
    "--------": "----------------------------------",
    "start": "npm run start:dev",
    "start:dev": "npm run build:tsc && run-p build:tsc:watch start:nodemon",
    "start:nodemon": "nodemon -w ./dist/src --delay 0.5 ./dist/src",
    "start:prod": "node ./dist/src",
    "-------": "-----------------------------------",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "------": "------------------------------------",
    "lint": "eslint  .",
    "lint:fix": "eslint  . --fix"
  },
