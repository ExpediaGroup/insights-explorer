  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "3.292.0",
    "fs-extra": "11.2.0",
    "lodash": "4.17.21",
    "parsimmon": "1.18.1",
    "pino": "8.8.0",
    "pino-http": "8.3.3",
    "pino-pretty": "10.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/lodash": "4.14.202",
    "@types/node": "18.15.11",
    "@types/parsimmon": "1.10.9",
    "typescript": "4.6.4"
  }
}
