    "pino-pretty": "7.6.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.36",
    "@types/parsimmon": "1.10.6",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4"
