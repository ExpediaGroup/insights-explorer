  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.22",
    "@types/parsimmon": "1.10.6",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
