  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "3.534.0",
    "fs-extra": "11.2.0",
    "lodash": "4.17.21",
    "parsimmon": "1.18.1",
    "pino": "8.18.0",
    "pino-http": "9.0.0",
    "pino-pretty": "10.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/lodash": "4.17.5",
    "@types/node": "20.11.27",
    "@types/parsimmon": "1.10.9",
    "typescript": "5.6.3"
  }
}
