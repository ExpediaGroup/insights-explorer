{
  "name": "insights-explorer",
  "version": "0.188.2",
  "description": "Insights Explorer",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:ExpediaGroup/insights-explorer.git"
  },
  "keywords": [
    "insights-explorer",
    "insights"
  ],
  "author": {
    "name": "Expedia Group",
    "email": "oss@expediagroup.com"
  },
  "license": "TBD",
  "workspaces": [
    "./packages/*"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.2",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.176",
    "@types/node": "14.17.34",
    "@types/npm-license-crawler": "0.2.1",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "38.0.1",
    "fs-extra": "10.0.0",
    "graphql": "15.5.3",
    "husky": "4.3.8",
    "jest": "27.0.6",
    "lerna": "3.22.1",
    "lodash": "4.17.21",
    "npm-license-crawler": "0.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "ts-jest": "27.0.4",
    "ts-node": "9.1.1",
    "typescript": "4.4.4"
  },
  "scripts": {
    "license:thirdparty": "ts-node scripts/third-party.ts",
    "prune:production": "lerna run prune:production --parallel",
    "getVersion": "node -p \"require('./package.json').version\"",
    "bootstrap": "lerna bootstrap",
    "---------": "----------------------------------",
    "start:backend": "npm start --prefix packages/backend",
    "start:backend:prod": "npm run start:prod --prefix packages/backend",
    "start:convertbot:prod": "npm run start:prod --prefix packages/convertbot",
    "start:frontend": "run-p generate:watch \"start --prefix packages/frontend\"",
    "start:frontend:nogen": "npm run start --prefix packages/frontend",
    "start:slackbot:prod": "npm run start:prod --prefix packages/slackbot",
    "start": "npm run generate && run-p start:frontend start:backend",
    "start:nogen": "run-p start:frontend:nogen start:backend",
    "-------": "-----------------------------------",
    "lint": "lerna run lint --parallel",
    "lint:fix": "lerna run lint:fix --parallel",
    "------": "------------------------------------",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch \"packages/frontend/src/**/*.ts\"",
    "--------": "------------------------------------",
    "clean": "lerna run clean --parallel",
    "clean:deep": "lerna run clean:deep --parallel && rm -rf node_modules && rm -f package-lock.json",
    "build": "run-s build:backend generate lint build:tsc && npm run build:frontend:react",
    "build:tsc": "tsc --build --incremental --verbose",
    "build:backend": "npm run build --prefix packages/backend",
    "build:frontend:react": "npm run build:react --prefix packages/frontend",
    "-----": "------------------------------------",
    "test": "lerna run test --parallel"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
